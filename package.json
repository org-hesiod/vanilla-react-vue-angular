{
  "name": "@tgkr/components",
  "version": "0.1.0",
  "description": "UI Components",
  "main": "lib/index.js",
  "scripts": {
    "docs:json": "compodoc -p ./internals/storybook/angular/tsconfig.json -e json -d ./internals/storybook/angular/",
    "storybook:vanilla": "node ./node_modules/@storybook/web-components/dist/server --static-dir ./node_modules,./source/vanilla --port 9000 --config-dir ./internals/storybook/vanilla",
    "storybook:react": "node ./node_modules/@storybook/react/dist/server -p 9001 -c ./internals/storybook/react/",
    "storybook:vue": "node ./node_modules/@storybook/vue/dist/server -p 9002 -c ./internals/storybook/vue/",
    "storybook:angular": "npm run docs:json && node ./node_modules/@storybook/angular/dist/server -p 9003 -c ./internals/storybook/angular/",
    "build-storybook:vanilla": "cross-env NODE_ENV=production node ./node_modules/@storybook/web-components/dist/server/build --config-dir ./internals/storybook/vanilla --output-dir ./lib/storybook/vanilla",
    "build-storybook:react": "cross-env NODE_ENV=production node ./node_modules/@storybook/react/dist/server/build -c ./internals/storybook/react/ -o ./lib/storybook/react",
    "build-storybook:vue": "cross-env NODE_ENV=production node ./node_modules/@storybook/vue/dist/server/build -c ./internals/storybook/vue/ -o ./lib/storybook/vue",
    "build-storybook:angular": "cross-env NODE_ENV=production npm run docs:json && node ./node_modules/@storybook/angular/dist/server/build -c ./internals/storybook/angular/ -o ./lib/storybook/angular",
    "release": "./node_modules/.bin/release-it --no-npm.publish --git.tagName='v${version}' --github.release"
  },
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.4.1"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgkr/components.git"
  },
  "keywords": [
    "togakure",
    "ui",
    "vanilla",
    "react",
    "vue",
    "angular"
  ],
  "author": "Hemerson Vianna <hemerson.lourenco@gmail.com> (https://nerdcalistenico.com.br)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgkr/components/issues"
  },
  "homepage": "https://github.com/tgkr/components#readme",
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.7",
    "@angular/cli": "10.1.7",
    "@angular/compiler-cli": "10.1.6",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@compodoc/compodoc": "1.1.11",
